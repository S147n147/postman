package test;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class data_provider {

	WebDriver driver;
	@BeforeClass
	public void setup() throws InterruptedException {
	System.setProperty("webdriver.chrome.driver","H:\\it\\selenium\\chromedriver_win32\\chromedriver.exe");
	driver = new ChromeDriver();
	driver.manage().window().maximize();
	Thread.sleep(1000);
	driver.get("https://login.wordpress.org/");
	Thread.sleep(1000);
	}
	@Test(dataProvider="testdata")
	public void getdata(String un, String pw) {
		
	}
	
	@DataProvider(name="testdata")
	public String[][] initdata() {
		return new String[][] { { "shubham", "9689152628" }, { "shyam", "9689" }, { "ram", "4563" } };
	}
}
